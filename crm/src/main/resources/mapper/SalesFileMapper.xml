<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kaishengit.mapper.SalesFileMapper">
<cache/>
    <select id="findSalesFileBySalesId" resultType="SalesFile">
        SELECT * FROM t_sales_file WHERE salesid = #{salesid}
    </select>
    <select id="findSalesFileById" resultType="com.kaishengit.pojo.SalesFile">
        SELECT * FROM t_sales_file WHERE id = #{id}
    </select>
    <insert id="save">
        INSERT INTO t_sales_file(name, filename, contenttype,`size`, salesid)
        VALUES (#{name},#{filename},#{contenttype},#{size},#{salesid})
    </insert>
</mapper>